export _start() nat = {
  ibrk 0
  (1 << 20) is big
  cast @byte alloc big is p
  { p is q
    { 1 for _
      read q big is n
      { (n == -1) then
        exit 1 }
      (q = &q^n)
      (big = big - n)
      n }
    (q@ = cast byte 0) }
  cast K 0 is k
  tokens &k p
  decls &k cast O 0
  exit 0
}
