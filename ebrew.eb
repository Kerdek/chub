export _start() nat = {
  cast @none brk 0 is curbrk
  brk ((1 << 24) + cast nat curbrk)
  cast @byte alloc (1 << 20) &curbrk is p
  ({ p for p
    read p (1 << 20) is n
    n then
    &p^n }@ = 0)
  cast @byte alloc (1 << 23) &curbrk is q
  scratch Ctx ctx
  (ctx.curbrk = curbrk)
  (ctx.p = p)
  (ctx.q = q)
  (ctx.line = 1)
  (ctx.col = 1)
  (ctx.o = 0)
  token &ctx
  decls &ctx
  { q for q
    write q (ctx.q - q) is n
    n then
    &q^n }
  exit 0
}
