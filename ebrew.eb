export _start() nat = {
  cast @none brk 0 is curbrk
  brk ((1 << 22) + cast nat curbrk)
  cast @byte alloc (1 << 20) &curbrk is p
  ({ p for q
    read q (1 << 20) is n
    n then
    &q^n }@ = 0)
  scratch Ctx ctx
  (ctx.curbrk = curbrk)
  (ctx.p = p)
  (ctx.line = 1)
  (ctx.col = 1)
  (ctx.o = 0)
  token &ctx
  decls &ctx
  exit 0
}
