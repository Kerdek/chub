export _start() nat = {
  ibrk 0
  (1 << 20) is big
  cast @byte alloc big is p
  gt0 ".data"
  gt0 ".globl .scurbrk"
  gt0 ".weak .scurbrk"
  gt0 ".scurbrk:.quad 0"
  scratch Ctx ctx
  (read &ctx.c0 1)
  (ctx.c1 = cast byte -1)
  (ctx.p = p)
  (ctx.p@ = ctx.c0)
  (ctx.line = 1)
  (ctx.col = 1)
  (ctx.o = 0)
  token &ctx
  decls &ctx
  exit 0
}
